{
  "host": "localhost",
  "port": 80,
  "cacheTime": "20m",
  "cachePath": ".jr/cache/",
  "substitutePath": ".jr/subs/",
  "jrPort": 8000,
  "proxyTimeout": "6s",
  "rules": [
    {
      "href" : "api/users.do",
      "subs" : "jrs/users.jrs"
    },
    {
      "href" : "api/save-user.do",
      "subs" : "jrs/save-user.jrs"
    },
    {
      "href" : "api/upload.do",
      "subs" : "jrs/upload.jrs"
    },
    {
      "href" : "api/post.do",
      "subs" : "api/post.json"
    },
    {
      "href" : "api/user.do",
      "subs" : "api/user.json"
    }
  ]
}
